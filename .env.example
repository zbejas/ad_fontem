# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# YouTube API Configuration
YOUTUBE_API_KEY=your_youtube_api_key_here

# Ollama LLM Configuration (optional)
OLLAMA_ENABLED=false
OLLAMA_URL=http://ollama:11434
OLLAMA_MODEL=llama3.1:8b
OLLAMA_KEEP_ALIVE=0
# OLLAMA_PROMPT is optional - if not set, will use default from system_prompt.txt

# Set to true to skip regex matching and only use Ollama for link extraction
# This is quite useful since regex matching can be unreliable for complex descriptions, and I am bad at writing regex
# Even this regex was written by AI and modified on the fly when needed
OLLAMA_ONLY=true

# Debug mode (set to true for verbose logging)
DEBUG=false
